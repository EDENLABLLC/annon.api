{
  "skip_files": [
    "test/*",
    "lib/annon_api/release_tasks.ex"
  ],
  "custom_stop_words": [
    "field",
    "has_one",
    "has_many",
    "embeds_one",
    "embeds_many",
    "schema",
    "send",
    "def init(.+\/\/.+).+do"
  ],
  "coverage_options": {
    "treat_no_relevant_lines_as_covered": true,
    "output_dir": "cover/",
  }
}
